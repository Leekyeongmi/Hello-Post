{
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "rules": {
    "require-jsdoc": 0,
    "react/react-in-jsx-scope": "off",
    "prettier/prettier": "error",
    "react/prop-types": [
      "<enabled>",
      {
        "ignore": "<ignore>",
        "customValidators": "<customValidator>",
        "skipUndeclared": "<skipUndeclared>"
      }
    ]
  }
}
